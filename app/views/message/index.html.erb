<div class="container">
  <%= render "layouts/sidenav" %>

  <div class="content">
    <div class="square-container">
      <% @messages.each do |message| %>
        <div class="square">
          <%= link_to message_path(message) do %>
            <div class="content"><%= message.content %></div>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>
