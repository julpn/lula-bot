<div class="header">
  <div id="logo">
    <%= image_tag("logo.png", size: "250x73", alt: "lula logo") %>
  </div>
  <div id="sign-in-links">
    <% if user_signed_in? %>
      <%= link_to('Logout', destroy_user_session_path, method: :delete) %>
    <% else %>
      <%= link_to('Login', new_user_session_path)  %>
    <% end %>
  </div>
</div>
